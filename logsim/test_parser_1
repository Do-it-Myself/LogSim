/ Combinational logic /
SWITCH {
A: 1;
B: 0;
C: 0;
}
DEVICE {
G1: NAND, 16;
G2: XOR;
G3: NAND, 3;
}


CLOCK {
CLK1: 2;
}

/ Initial monitoring points /
MONITOR {
S1: G1;
f: G3;
}

/ Internal connections of the circuit /
CONNECTION {
A > G1.I1;
B > G2.I1;
CLK1 > G2.I2;
G1 > G3.I1;
G2 > G3.I2;
C > G3.I3;
}