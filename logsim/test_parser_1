/ Combinational logic /
DEVICE {
G1: NAND, 2;
G3: NAND, 3;
G2: XOR;
}

/ Inputs /
SWITCH {
A: 1;
B: 0;
C: 0;
}

CLOCK {
CLK1: 2;
}

/ Initial monitoring points /
MONITOR {
S1: G1;
f: G3;
}

/ Internal connections of the circuit /
CONNECTION {
A > G1.I1;
A > G1.I2;
B > G2.I1;
CLK1 > G2.I2;
G1 > G3.I1;
B > G3.I3;
G2 > G3.I2;
}